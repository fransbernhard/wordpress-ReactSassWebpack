{
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime",
    "syntax-dynamic-import"
  ],
  "presets": [
      ["es2015", { "modules": false }],
      "react",
      "stage-0"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-0"],
      "plugins": [
        "dynamic-import-node",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
